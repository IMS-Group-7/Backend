datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]

  // binaryTargets = ["native", "linux-musl-arm64-openssl-1.1.x"]
}

