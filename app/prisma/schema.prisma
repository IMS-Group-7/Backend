model Session {
  id         String       @id @default(cuid())
  startTime  DateTime
  endTime    DateTime?
  coordinate Coordinate[]
}

model Coordinate {
  id        String    @id @default(cuid())
  session   Session   @relation(fields: [sessionId], references: [id])
  sessionId String
  x         Int
  y         Int
  type      String
  timestamp DateTime
  obstacle  Obstacle?
}

model Obstacle {
  id           String     @id @default(cuid())
  coordinate   Coordinate @relation(fields: [coordinateId], references: [id])
  coordinateId String     @unique
  imagePath    String
  object       String
}