datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "linux-musl-arm64-openssl-1.1.x"
}

model Mower {
  id                  Int                  @id @default(autoincrement())
  serialNumber        String               @unique
  Coordinate         Coordinate[]
  CollisionAvoidance CollisionAvoidance[]
}

model Coordinate {
  id          Int     @id @default(autoincrement())
  mower       Mower   @relation(fields: [mowerId], references: [id])
  mowerId     Int
  x           Int
  y           Int
  is_boundary Boolean
  timestamp   String
}

model CollisionAvoidance {
  id        Int    @id @default(autoincrement())
  mower     Mower  @relation(fields: [mowerId], references: [id])
  mowerId   Int
  x         Int
  y         Int
  imagePath String
  object    String
  timestamp String
}
